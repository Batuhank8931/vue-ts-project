ry=\"childCategory\"\n        :deleteCategory=\"deleteCategory\"\n        :openUpdatePopup=\"openUpdatePopup\"\n      />\n    </ul>\n  </li>\n</template>\n\n<script>\nexport default {\n  props: {\n    category: Object,\n    deleteCategory: Function,\n    openUpdatePopup: Function\n  },\n  data() {\n    return {\n      showChildren: false\n    };\n  },\n  computed: {\n    categoryData() {\n      const { id, name, picture, parent_id, created_at, updated_at, children } = this.category;\n      return { id, name, picture, parent_id, created_at, updated_at, children };\n    }\n  },\n  methods: {\n    toggleChildren() {\n      this.showChildren = !this.showChildren;\n    }\n  }\n};\n</script>\n\n<style scoped>\n.category {\n  display: flex;\n  flex-direction: column;\n  background-color: #f0f0f0;\n  border: 1px solid #ccc;\n  margin-bottom: 4px;\n}\n\n.details {\n  padding: 8px;\n}\n\n.actions button {\n  margin-left: 8px;\n}\n\nul {\n  list-style-type: none;\n  margin-left: 16px;\n  padding: 0;\n}\n</style>\n"],"mappings":"AA+BA,eAAe;EACbA,KAAK,EAAE;IACLC,QAAQ,EAAEC,MAAM;IAChBC,cAAc,EAAEC,QAAQ;IACxBC,eAAe,EAAED;EACnB,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,MAAM;QAAEC,EAAE;QAAEC,IAAI;QAAEC,OAAO;QAAEC,SAAS;QAAEC,UAAU;QAAEC,UAAU;QAAEC;MAAS,IAAI,IAAI,CAACf,QAAQ;MACxF,OAAO;QAAES,EAAE;QAAEC,IAAI;QAAEC,OAAO;QAAEC,SAAS;QAAEC,UAAU;QAAEC,UAAU;QAAEC;MAAS,CAAC;IAC3E;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACX,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}